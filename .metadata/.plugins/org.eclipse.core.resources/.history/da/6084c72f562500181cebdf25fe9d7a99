package com.oops.inheritance.withsuperkeyword;

public class Lab442 {
	public static void main(String[] args) {

	}

}

class Hai2 {
	static int a = 10;
}

class Hello2 extends Hai2 {
	static int a = 20;

	void show() {
		int a = 30;
		System.out.println(a);
		System.out.println(this.a);
		System.out.println(super.a);
	}
}
