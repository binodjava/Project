package com.oops.inheritance.withsuperkeyword;

public class Lab441 {
	public static void main(String[] args) {

	}

}

class Hai1 {
}

class Hello1 extends Hai1 {
	int a = 20;

	void show() {
		int a = 30;
		System.out.println(a);// 30
		System.out.println(this.a);// 20
		System.out.println(super.a);// there is no var a is present in super
									// class so compile time error
	}
}
