package com.oops.inheritance;

public class Lab423 {
	public static void main(String[] args) {
		B1 bobj=new B1();
		bobj.m1();
		bobj.m2();
		//bobj.m3(); can't be called
		
	}

}

class A1 {
	void m1() {
		System.out.println("A->m1");
	}
}

class B1 extends A1 {
	void m2() {
		System.out.println("B->m2");
	}
}

class C1 extends B1 {
	void m3() {
		System.out.println("C->m3");
	}
}
